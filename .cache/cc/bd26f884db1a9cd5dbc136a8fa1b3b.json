 window.clearInterval(player.id);\r\n      return;\r\n    }\r\n    // 在页面上写css\r\n    player.ui.demo.innerText = string.substring(0, player.n);\r\n    // 在结构中写css\r\n    player.ui.demo2.innerHTML = string.substring(0, player.n);\r\n    player.ui.demo.scrollTop = player.ui.demo.scrollHeight;\r\n  },\r\n  // 播放\r\n  play: () => {\r\n    // console.log(\"play\");\r\n    player.id = setInterval(player.run, player.time);\r\n  },\r\n  //暂停\r\n  pause: () => {\r\n    window.clearInterval(player.id);\r\n  },\r\n  // 慢速\r\n  slow: () => {\r\n    player.pause();\r\n    player.time = 300;\r\n    player.play();\r\n  },\r\n  // 中速\r\n  normal: () => {\r\n    player.pause();\r\n    player.time = 100;\r\n    player.play();\r\n  },\r\n  // 快速\r\n  fast: () => {\r\n    player.pause();\r\n    player.time = -100;\r\n    player.play();\r\n  },\r\n};\r\n// 调用初始化\r\nplayer.init();\r\n "},"lineCount":null}},"error":null,"hash":"85dbcf30a63d938d1e66a88e91543d8c","cacheData":{"env":{}}}